import { Injectable } from '@angular/core';

@Injectable({
    providedIn: 'root'
  })

export class LoggerService {

    user:any = {};
    private TOKEN_KEY = 'token';

    get token() {
        return localStorage.getItem(this.TOKEN_KEY);
    }

    setCurrentUser(currentUser){
        this.user=currentUser;
    }

    getCurrentUser(){    
        return this.user;
    }
}
